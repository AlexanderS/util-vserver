enabled=0
gpgcheck=1
gpgkey=http://download.fedoraproject.org/pub/fedora/linux/releases/14/Everything/$basearch/os/RPM-GPG-KEY-fedora-$basearch

[updates-source]
name=Fedora 14 - Updates Source
failovermethod=priority
#baseurl=http://download.fedoraproject.org/pub/fedora/linux/updates/14/SRPMS/
